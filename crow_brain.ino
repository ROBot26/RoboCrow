const int crowEye1Pin = 2;
const int crowEye2Pin = 3;
const int IRLedPin = 4;
const int IRSensorPin = 7;
float sensorValue = 0;
const long freq = 38000;

// irDetect from Parallax BOE documentation.
int irDetect(int irLedPin, int irReceiverPin, long frequency)
{
 tone(irLedPin, frequency, 8); // IRLED 38 kHz for at least 1 ms
 delay(1); // Wait 1 ms
 int ir = digitalRead(irReceiverPin); // IR receiver -> ir variable
 delay(1); // Down time before recheck
 return ir; // Return 1 no detect, 0 detect
}

// What happens when crow senses something
void activateCrow(){
  Serial.println("I see you");
  startPlayback(sample, sizeof(sample));
  digitalWrite(crowEye1Pin, HIGH);
  digitalWrite(crowEye2Pin, HIGH);
  startPlayback(sample, sizeof(sample));
  delay(2000);
  digitalWrite(crowEye1Pin, LOW);
  digitalWrite(crowEye2Pin, LOW);
  delay(2000);
}

void setup() {
  pinMode(crowEye1Pin, OUTPUT);
  pinMode(crowEye2Pin, OUTPUT);
  pinMode(IRLedPin, OUTPUT);
  pinMode(IRSensorPin, INPUT);
  //digitalWrite(IRSensorPin, HIGH);
  //Serial.begin(9600);
}

void loop() {
  
  
  int sensorValue = irDetect(IRLedPin,IRSensorPin,freq);
  //Serial.println(sensorValue);
  if(!sensorValue){
    activateCrow();
  }
  

}

 



